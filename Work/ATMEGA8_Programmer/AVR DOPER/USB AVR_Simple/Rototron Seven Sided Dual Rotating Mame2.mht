		else if( img.fade.state =3D=3D "FADE_UP_DOWN"
			|| img.fade.state =3D=3D "FADE_DOWN")
		{
			img.fade.upStep  =3D step;
			img.fade.state =3D "FADE_UP";
		}
	}
}
/*******************************************************************
*
* Function    : fadeDownImg
*
* Description : Finds the image in the document and calls JSFX.fadeDown
*
*****************************************************************/
JSFX.fadeDownImg =3D function(imgName, step)
{
	if(document.layers || window.opera)
		return;

	img =3D document.images[imgName];
	if(img)
		JSFX.fadeDown(img, step);
}
/*******************************************************************
*
* Function    : fadeDown
*
* Description : This function is based on the turn_off function
*		      of animate2.js (animated rollovers from www.roy.whittle.com).
*		      Each zoom object is given a state.=20
*			OnMouseOut the state is switched depending on the current state.
*			Current state -> Switch to
*			=
=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=
=3D=3D
*			ON		->	FADE_DOWN.
*			FADE_UP	->	FADE_UP_DOWN.
*****************************************************************/
JSFX.fadeDown =3D function(img, step)
{
	if(img)
	{
		if(!step) step=3DJSFX.FadeImageAutoDown;

		if(img.fade.state=3D=3D"ON")
		{
			img.fade.downStep  =3D step;
			img.fade.state=3D"FADE_DOWN";
			JSFX.startImageFading();
		}
		else if(img.fade.state =3D=3D "FADE_UP")
		{
			img.fade.downStep  =3D step;
			img.fade.state=3D"FADE_UP_DOWN";
		}
	}
}
/*******************************************************************
*
* Function    : startImageFading
*
* Description : This function is based on the start_animating() function
*	        	of animate2.js (animated rollovers from www.roy.whittle.com).
*			If the timer is not currently running, it is started.
*			Only 1 timer is used for all objects
*****************************************************************/
JSFX.startImageFading =3D function()
{
	if(!JSFX.FadeImageRunning)
		JSFX.FadeImageAnimation();
}

/*******************************************************************
*
* Function    : FadeImageAnimation
*
* Description : This function is based on the Animate function
*		    of animate2.js (animated rollovers from www.roy.whittle.com).
*		    Each object has a state. This function
*		    modifies each object and (possibly) changes its state.
*****************************************************************/
JSFX.FadeImageAnimation =3D function()
{
	JSFX.FadeImageRunning =3D false;
	for(i=3D0 ; i<document.images.length ; i++)
	{
		var img =3D document.images[i];
		if(img.fade)
		{
			if(img.fade.state =3D=3D "FADE_UP")
			{
				img.fade.index+=3Dimg.fade.upStep;
				if(img.fade.index > 100)
					img.fade.index =3D 100;

				if(img.filters)
					img.filters.alpha.opacity =3D img.fade.index;
				else
					img.style.MozOpacity =3D img.fade.index/101;

				if(img.fade.index =3D=3D 100)
					img.fade.state=3D"ON";
				else
					JSFX.FadeImageRunning =3D true;
			}
			else if(img.fade.state =3D=3D "FADE_UP_DOWN")
			{
				img.fade.index+=3Dimg.fade.upStep;
				if(img.fade.index > 100)
					img.fade.index =3D 100;

				if(img.filters)
					img.filters.alpha.opacity =3D img.fade.index;
				else
					img.style.MozOpacity =3D img.fade.index/101;
=09
				if(img.fade.index =3D=3D 100)
					img.fade.state=3D"FADE_DOWN";
				JSFX.FadeImageRunning =3D true;
			}
			else if(img.fade.state =3D=3D "FADE_DOWN")
			{
				img.fade.index-=3Dimg.fade.downStep;
				if(img.fade.index < img.fade.minOpacity)
					img.fade.index =3D img.fade.minOpacity;

				if(img.filters)
					img.filters.alpha.opacity =3D img.fade.index;
				else
					img.style.MozOpacity =3D img.fade.index/101;

				if(img.fade.index =3D=3D img.fade.minOpacity)
					img.fade.state=3D"OFF";
				else
					JSFX.FadeImageRunning =3D true;
			}
		}
	}
	/*** Check to see if we need to animate any more frames. ***/
	if(JSFX.FadeImageRunning)
		setTimeout("JSFX.FadeImageAnimation()", 40);
}

