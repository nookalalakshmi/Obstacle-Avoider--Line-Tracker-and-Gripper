function WeekNbr(n)=20
{
  // Algorithm used:
  // From Klaus Tondering's Calendar document (The Authority/Guru)
  // hhtp://www.tondering.dk/claus/calendar.html
  // a =3D (14-month) / 12
  // y =3D year + 4800 - a
  // m =3D month + 12a - 3
  // J =3D day + (153m + 2) / 5 + 365y + y / 4 - y / 100 + y / 400 - =
32045
  // d4 =3D (J + 31741 - (J mod 7)) mod 146097 mod 36524 mod 1461
  // L =3D d4 / 1460
  // d1 =3D ((d4 - L) mod 365) + L
  // WeekNumber =3D d1 / 7 + 1

  year =3D n.getFullYear();
  month =3D n.getMonth() + 1;
  if (startAt =3D=3D 0) {
      day =3D n.getDate() + 1;
  }
  else {
      day =3D n.getDate();
  }

  a =3D Math.floor((14-month) / 12);
  y =3D year + 4800 - a;
  m =3D month + 12 * a - 3;
  b =3D Math.floor(y/4) - Math.floor(y/100) + Math.floor(y/400);
  J =3D day + Math.floor((153 * m + 2) / 5) + 365 * y + b - 32045;
  d4 =3D (((J + 31741 - (J % 7)) % 146097) % 36524) % 1461;
  L =3D Math.floor(d4 / 1460);
  d1 =3D ((d4 - L) % 365) + L;
  week =3D Math.floor(d1/7) + 1;

  return week;
}

